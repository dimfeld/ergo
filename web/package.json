{
  "name": "ergo-web",
  "version": "0.0.1",
  "scripts": {
    "dev": "run-p dev:svelte dev:server",
    "dev:svelte": "svelte-kit dev",
    "dev:server": "cargo watch --ignore web -s 'cd web && pnpm generate-api-types && ../car run --release server | bunyan'",
    "build": "run-s generate-api-types build:svelte",
    "build:svelte": "svelte-kit build",
    "preview": "run-s clean build && SERVE_DIR=build ../car run --release server | bunyan",
    "preview:svelte": "svelte-kit preview",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest src",
    "test:watch": "run-s test -- --watch",
    "clean": "rimraf build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "generate-api-types": "rimraf ../schemas/*.json && ../car run --release dev make-json-schema && node generate_api_types.js",
    "generate-ts-defs": "node scripts/build-types.mjs"
  },
  "pnpm": {
    "overrides": {
      "@sveltejs/kit": "1.0.0-next.202",
      "@sveltejs/vite-plugin-svelte": "1.0.0-next.31",
      "vite": "~2.6"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@sveltejs/adapter-static": "~1.0.0-next.23",
    "@sveltejs/adapter-vercel": "~1.0.0-next.32",
    "@sveltejs/kit": "1.0.0-next.202",
    "@sveltejs/vite-plugin-svelte": "1.0.0-next.31",
    "@tailwindcss/forms": "~0.4.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/svelte": "^3.0.3",
    "@types/json-schema": "~7.0.9",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "broadcast-channel": "~4.8.0",
    "cssnano": "^5.0.10",
    "dotenv": "~10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "fast-glob": "~3.2.7",
    "jest": "^27.3.1",
    "jest-transform-css": "^2.1.0",
    "jsdom": "~19.0.0",
    "json-schema-to-typescript": "^10.1.5",
    "ky-universal": "~0.9.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.11",
    "postcss-nested": "~5.0.6",
    "prettier": "~2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "rimraf": "~3.0.2",
    "source-map-support": "^0.5.20",
    "svelte": "~3.44.1",
    "svelte-check": "^2.2.8",
    "svelte-jester": "^2.1.5",
    "svelte-loader": "^3.1.2",
    "svelte-portal": "~2.1.2",
    "svelte-preprocess": "~4.10.1",
    "svench": "~0.2.0-31",
    "tailwindcss": "~3.0.8",
    "tippy.js": "^6.3.7",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "uvu": "~0.5.2",
    "vite": "~2.7.9"
  },
  "type": "module",
  "dependencies": {
    "@codemirror/autocomplete": "~0.19.9",
    "@codemirror/closebrackets": "~0.19.0",
    "@codemirror/commands": "~0.19.6",
    "@codemirror/comment": "~0.19.0",
    "@codemirror/gutter": "~0.19.9",
    "@codemirror/highlight": "~0.19.6",
    "@codemirror/history": "~0.19.0",
    "@codemirror/lang-javascript": "~0.19.3",
    "@codemirror/lang-json": "~0.19.1",
    "@codemirror/language": "~0.19.7",
    "@codemirror/lint": "~0.19.3",
    "@codemirror/matchbrackets": "~0.19.3",
    "@codemirror/panel": "~0.19.1",
    "@codemirror/rangeset": "~0.19.4",
    "@codemirror/search": "~0.19.5",
    "@codemirror/state": "~0.19.6",
    "@codemirror/text": "~0.19.5",
    "@codemirror/theme-one-dark": "~0.19.1",
    "@codemirror/tooltip": "~0.19.10",
    "@codemirror/view": "~0.19.34",
    "@lezer/common": "~0.15.8",
    "@popperjs/core": "^2.10.2",
    "@sveltestack/svelte-query": "~1.5.0",
    "@typescript/vfs": "~1.3.5",
    "ajv": "~8.7.1",
    "date-fns": "~2.25.0",
    "ergo-wasm": "link:../wasm/pkg",
    "fast-deep-equal": "~3.1.3",
    "focus-svelte": "~0.3.4",
    "json5": "~2.2.0",
    "just-camel-case": "~6.0.1",
    "just-capitalize": "~3.0.0",
    "just-clone": "~5.0.0",
    "just-debounce-it": "~3.0.1",
    "just-once": "~2.0.0",
    "just-throttle": "~4.0.0",
    "just-zip-it": "~3.0.0",
    "ky": "~0.28.7",
    "lezer-json5": "~1.0.2",
    "localforage": "~1.10.0",
    "path-browserify": "~1.0.1",
    "resolve.exports": "~1.1.0",
    "rollup": "~2.62.0",
    "sorters": "~1.0.0",
    "typescript": "^4.5.4"
  },
  "jest": {
    "setupFilesAfterEnv": ["@testing-library/jest-dom/extend-expect", "./jest-setup.js"],
    "transform": {
      "^.+\\.svelte$": [
        "./node_modules/svelte-jester/dist/transformer.mjs",
        {
          "preprocess": true
        }
      ],
      "^.+\\.(j|t)s$": "ts-jest",
      "^.+\\.p?css$": "jest-transform-css"
    },
    "moduleFileExtensions": ["js", "ts", "svelte"],
    "extensionsToTreatAsEsm": [".ts", ".svelte"],
    "globals": {
      "ts-jest": {
        "diagnostics": false,
        "useESM": true
      }
    }
  }
}
